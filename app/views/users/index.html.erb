<% content_for :thead do %>
	<tr>
		<th style="width: 40%">Email</th>
		<th></th>
	</tr>
<% end %>

<% content_for :table do %>
	
	<% @users.each do |i| %>
		<tr>
			<td><%= i.email %> <%= i.account_id %></td>
			<td class="centered"><%= link_to "Edit", edit_item_path(i), :class => 'link' %>
					<%= link_to "Delete", item_path(i), :method => :delete, :confirm => "Are you sure?" %></td>
		</tr>
	<% end %>
<% end %>